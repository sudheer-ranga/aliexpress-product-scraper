{
  "name": "aliexpress-product-scraper",
  "version": "4.0.0",
  "description": "Get Aliexpress product details as a json reponse including feedbacks, variants, description, images, etc.,",
  "main": "index.js",
  "type": "module",
  "files": [
    "index.js",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "node --test tests/unit/*.test.js tests/integration/*.test.js",
    "test:unit": "node --test tests/unit/*.test.js",
    "test:integration": "node --test tests/integration/*.test.js",
    "smoke": "node ./scripts/smoke.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "lint-staged": {
