    "dropship",
    "dropshipping",
    "aliexpress-scraper",
    "aliexpress-product-scraper",
    "aliexpress-product-json",
    "product-scraper",
    "product-json",
    "scraper",
    "crawler",
    "spider"
  ],
  "author": "Sudheer Ranga",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "10.2.0",
    "cheerio": "1.2.0",
    "node-fetch": "3.3.2",
    "puppeteer": "24.37.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7"
  }
}
